{
  "name": "team-project",
  "version": "0.0.1",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "build": "tsc -p . --outDir dist",
    "database-setup": "node dist/database/setupDatabase.js",
    "load-CSV-data-into-DB": "node ./dist/database/csvToDatabase/loadData.js",
    "check-database-table-content": "node ./dist/database/checkTableContents.js",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test-ci": "jest --max_old_space_size=4096 --watchAll=false --ci --coverage --passWithNoTests --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://git.cardiff.ac.uk/c21052928/team-project.git"
  },
  "author": "Jack Richards, Isaac Vieira, Rhiannon Austin, Khalid Al Shamsi, Hamed Al Sarmi",
  "license": "ISC",
